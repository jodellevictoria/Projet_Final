            android:label="@string/title_listeutilcontent_detail"
            android:parentActivityName=".Private.ListeUtilContentListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="jvpg.cgodin.qc.ca.projetpldl.Private.ListeUtilContentListActivity" />
        </activity>
        <activity android:name=".Private.ModifierListActivity" />
        <activity android:name=".Private.ModifierMusiqueActivity" />
