            android:name=".Public.ListePubliqueContenuListActivity"
            android:label="@string/title_listepubliquecontenu_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Public.ListePubliqueContenuDetailActivity"
            android:label="@string/title_listepubliquecontenu_detail"
            android:parentActivityName=".Public.ListePubliqueContenuListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="jvpg.cgodin.qc.ca.projetpldl.Public.ListePubliqueContenuListActivity" />
        </activity>
        <activity
            android:name=".Private.MusiquesUtilListActivity"
            android:label="@string/title_musiquesutil_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Private.MusiquesUtilDetailActivity"
            android:label="@string/title_musiquesutil_detail"
            android:parentActivityName=".Private.MusiquesUtilListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
